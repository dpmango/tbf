{
  "name": "tbf",
  "version": "0.1.0",
  "author": "Sergey Khmelevskoy <sergey@khmelevskoy.co> (https://github.com/dpmango/)",
  "private": true,
  "scripts": {
    "start": "env-cmd -f ./.env.dev.json cross-env craco start",
    "build": "env-cmd -f ./.env.prod.json cross-env GENERATE_SOURCEMAP=false craco build",
    "build:test": "env-cmd -f ./.env.dev.json cross-env GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "lint": "bash bin/lint.sh",
    "lint:js:fix": "eslint './src/**/*.{js,jsx}' --fix",
    "lint:css:fix": "stylelint './src/**/*.{css,less,scss,sass}' --fix",
    "deploy": "yarn build:test && cp build/index.html build/200.html && surge --project ./build --domain tbf.surge.sh",
    "eject": "react-scripts eject",
    "prod": "clear && env-cmd -f ./.env.prod.json cross-env GENERATE_SOURCEMAP=false craco build --stats --prod && rsync --delete -avzi build/* root@nbf:/var/www/buzzier_front"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{css,less,scss}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@googlemaps/react-wrapper": "^1.1.20",
    "@googlemaps/typescript-guards": "^1.0.4",
    "aos": "^3.0.0-beta.6",
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "fast-equals": "^2.0.4",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "mobx": "^6.3.3",
    "mobx-persist-store": "^1.0.4",
    "mobx-react": "^7.2.0",
    "mobx-react-lite": "^3.2.3",
    "mobx-utils": "^6.0.4",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-autosize-textarea": "^7.1.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-input-mask": "^2.0.4",
    "react-modal": "^3.14.3",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-select": "^5.0.0",
    "react-spinners": "^0.11.0",
    "react-toast-notifications": "^2.5.1",
    "react-tooltip": "^4.2.21",
    "retry-axios": "^2.6.0",
    "sass": "^1.48.0",
    "swiper": "^7.2.0",
    "uuid": "^8.3.2",
    "viewport-units-buggyfill": "^0.6.2"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.26.0",
    "husky": "4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.5.4",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  }
}
